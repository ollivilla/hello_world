# LIST METHODS

lst = [1, 82, -6, 4, 3, 8]
print lst[2]

# prindib viimase elemendi
print lst[-1]

# kontrollib, kas 4 on listis olemas
if 4 in lst:
    print "4 is there"
    

print lst.index(8)
print lst.index(4)

# muudab pos 2. oleva elemendi 5-ks
lst[2] = 5

# lisab listi l√µppu
lst.append(632)
print lst

# eemaldab listist viimase
lst.pop()
print lst

# eemaldab listist pos. 2
lst.pop(2)
print lst
